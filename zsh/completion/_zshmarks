#compdef jump unmark

_jump() {
    MARKS=$(find "$MARKDIR" "$DROPBOX_MARKDIR" -type f -exec basename {} \;)
    _arguments "1: :($MARKS)"
}

_unmark() {
    MARKS=$(find "$MARKDIR" "$DROPBOX_MARKDIR" -type f -exec basename {} \;)
    _arguments "*: :($MARKS)"
}

_zshmarks_comp() {
    case "$service" in
        jump)
            _jump "$@"
            ;;
        unmark)
            _unmark "$@"
            ;;
    esac
}

_zshmarks_comp "$@"
