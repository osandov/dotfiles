#compdef findmark jump readmark unmark

_zshmarks_comp() {
    MARKS=$(marks -n)
    case "$service" in
        findmark)
            ;&
        jump)
            _arguments "1: :($MARKS)"
            ;;
        readmark)
            ;&
        unmark)
            _arguments "*: :($MARKS)"
            ;;
    esac
}

_zshmarks_comp "$@"
