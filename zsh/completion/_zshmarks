#compdef findmark jump readmark unmark

_zshmarks_comp() {
    MARKS=$(find ${^MARKDIRS}/.marks -type f -exec basename {} \;)
    case "$service" in
        findmark)
            ;&
        jump)
            _arguments "1: :($MARKS)"
            ;;
        readmark)
            ;&
        unmark)
            _arguments "*: :($MARKS)"
            ;;
    esac
}

_zshmarks_comp "$@"
